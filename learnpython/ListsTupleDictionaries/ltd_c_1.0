# convert a number into a string of individual digits in words
num_to_word = {"0": "Zero", "1": "One", "2": "Two", "3": "Three", "4": "Four", "5": "Five", "6": "Six", "7": "Seven", "8": "Eight", "9": "Nine"}
phrase = ""
num = input("Enter any number: ")
for digit in num:
    word = num_to_word.get(digit, "?")
    phrase += word + " "
print(f"Your number in words: {phrase}")
